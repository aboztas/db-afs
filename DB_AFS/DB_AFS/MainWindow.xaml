<Window x:Class="DB_AFS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Datenbank AFS" Height="200" Width="200" MinHeight="200" MinWidth="200" WindowStartupLocation="CenterScreen" Icon="Icon/Places-server-database-icon.png">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button Name="ButtonAdem" Content="Adem" Grid.Row="0" Click="ButtonAdem_Click"/>
        <Button Name="ButtonMo" Content="Mo" Grid.Row="1" Click="ButtonMo_Click"/>
        <Button Name="ButtonFatih" Content="Fatih" Grid.Row="2" Click="ButtonFatih_Click"/>
    </Grid>
</Window>
